{
  "intents": [
    {
      "tag": "greeting",
      "patterns": [
        "Hi",
        "Hello",
        "Good morning",
        "Good afternoon",
        "Good evening",
        "Hey",
        "Howdy",
        "Is anyone there?",
        "Greetings"
      ],
      "responses": [
        "Hello, thanks for visiting",
        "Good to see you again",
        "Hi there, how can I help?"
      ]
    },
    {
      "tag": "goodbye",
      "patterns": [
        "Bye",
        "See you later",
        "Goodbye",
        "Take care",
        "Catch you later"
      ],
      "responses": [
        "See you later, thanks for visiting",
        "Have a nice day",
        "Bye! Come back again soon."
      ]
    },
    {
      "tag": "thanks",
      "patterns": [
        "Thanks",
        "Thank you",
        "That's helpful",
        "I appreciate it",
        "Much obliged"
      ],
      "responses": ["Happy to help!", "Any time!", "My pleasure"]
    },
    {
      "tag": "hours",
      "patterns": [
        "What hours are you open?",
        "What are your hours?",
        "When are you open?",
        "Opening hours",
        "Business hours"
      ],
      "responses": [
        "We're open every day 09:00am - 05:00pm",
        "Our hours are 09:00am - 05:00pm every day"
      ]
    },
    {
      "tag": "products",
      "patterns": [
        "Which products do you have?",
        "What kinds of products are there?",
        "What do you sell?",
        "Product list",
        "Catalog"
      ],
      "responses": [
        "We have laptops, desktops, and servers from various brands."
      ]
    },
    {
      "tag": "payments",
      "patterns": [
        "Do you take credit cards?",
        "Do you accept Mastercard?",
        "Are you cash only?",
        "Payment methods",
        "How can I pay?"
      ],
      "responses": [
        "We accept VISA, Mastercard, Valu, and Meza",
        "We accept most major credit cards"
      ]
    },
    {
      "tag": "opentoday",
      "patterns": [
        "Are you open today?",
        "When do you open today?",
        "What are your hours today?",
        "Opening hours today",
        "Business hours today"
      ],
      "responses": [
        "We're open every day from 09:00am - 05:00pm",
        "Our hours are 09:00am - 05:00pm every day"
      ]
    },
    {
      "tag": "ask_name",
      "patterns": [
        "What's your name?",
        "Can I know your name?",
        "May I know your name?",
        "Your name?",
        "Who are you?"
      ],
      "responses": ["Sure, my name is Chatbot. What's your first name?"]
    },
    {
      "tag": "first_name",
      "patterns": [
        "My first name is {first_name}",
        "I am {first_name}",
        "It's {first_name}",
        "Call me {first_name}"
      ],
      "responses": ["Nice to meet you, {first_name}! What's your last name?"]
    },
    {
      "tag": "last_name",
      "patterns": [
        "My last name is {last_name}",
        "I am {last_name}",
        "It's {last_name}",
        "My surname is {last_name}"
      ],
      "responses": [
        "Great, {first_name} {last_name}! How can I help you today?"
      ]
    },
    {
      "tag": "order_tracking",
      "patterns": [
        "Can I track my order?",
        "What's the status of my order?",
        "Track order ID {order_id}",
        "Order status",
        "Where is my order?"
      ],
      "responses": ["Please provide your order tracking ID."]
    }
  ]
}
